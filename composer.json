{
  "name": "user/primes",
  "description": "Prime Number Multiplication Table",
  "type": "project",
  "authors": [
    {
      "name": "Tsanko Marinov",
      "email": "ts.marinov@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-zlib": "*",
    "symfony/process": "^7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
